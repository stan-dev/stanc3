functions {
  real[] simple_SIR(real t, real[] y, real[] theta, real[] x_r, int[] x_i) {
    real dydt[4];
    dydt[1] = -theta[1] * y[4] / (y[4] + theta[2]) * y[1];
    dydt[2] = theta[1] * y[4] / (y[4] + theta[2]) * y[1] - theta[3] * y[2];
    dydt[3] = theta[3] * y[2];
    dydt[4] = theta[4] * y[2] - theta[5] * y[4];
    return dydt;
  }
}
data {
  int<lower=0> N_t;
  real t[N_t];
  real y0[4];
  int stoi_hat[N_t];
  real B_hat[N_t];
