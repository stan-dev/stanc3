data {
  int<lower=2> K;
  int<lower=2> V;
  int<lower=1> M;
  int<lower=1> N;
  int<lower=1, upper=V> w[N];
  int<lower=1, upper=M> doc[N];
  vector<lower=0>[V] beta;
}
parameters {
  vector[K] mu;
  corr_matrix[K] Omega;
  vector<lower=0>[K] sigma;
  vector[K] eta[M];
  simplex[V] phi[K];
}
transformed parameters {
  simplex[K] theta[M];
  cov_matrix[K] Sigma;
  for (m in 1 : M) 
    theta[m] <- softmax(eta[m]);
  for (m in 1 : K) {
    Sigma[m, m] <- sigma[m] * sigma[m] * Omega[m, m];
    for (n in (m + 1) : K) {
      Sigma[m, n] <- sigma[m] * sigma[n] * Omega[m, n];
      Sigma[n, m] <- Sigma[m, n];
    }
  }
}
model {
  for (k in 1 : K) 
    phi[k] ~ dirichlet(beta);
  mu ~ normal(0, 5);
  Omega ~ lkj_corr(2.0);
  sigma ~ cauchy(0, 5);
  for (m in 1 : M) 
    eta[m] ~ multi_normal(mu, Sigma);
  for (n in 1 : N) {
    real gamma[K];
    for (k in 1 : K) 
      gamma[k] <- log(theta[doc[n], k]) + log(phi[k, w[n]]);
    increment_log_prob(log_sum_exp(gamma));
  }
}


