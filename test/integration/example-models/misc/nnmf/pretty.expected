data {
  int<lower=0> T;
  int<lower=0> I;
  int<lower=0> K;
  matrix<lower=0.0>[T, I] X;
  real<lower=0> sigma[I];
}
transformed data {
  real<lower=0> g_bar;
  real<lower=0> g_sigma;
  vector[T] temp;
  for (t in 1 : T) 
    temp[t] <- log(sum(X[t]));
  g_bar <- mean(temp);
  g_sigma <- sd(temp);
}
parameters {
  matrix<lower=0>[T, K] G;
  simplex[I] F[K];
}
model {
  for (t in 1 : T) 
