  for (k in 1 : K) 
    theta[k] ~ dirichlet(alpha);
  for (k in 1 : K) 
    phi[k] ~ dirichlet(beta);
  for (t in 1 : T) 
    w[t] ~ categorical(phi[z[t]]);
  for (t in 2 : T) 
    z[t] ~ categorical(theta[z[t - 1]]);
