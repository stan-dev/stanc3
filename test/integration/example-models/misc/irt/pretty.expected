data {
  int<lower=1> J;
  int<lower=1> K;
  int<lower=1> N;
  int<lower=1, upper=J> jj[N];
  int<lower=1, upper=K> kk[N];
  int<lower=0, upper=1> y[N];
}
parameters {
  real delta;
  real alpha[J];
  real beta[K];
}
model {
  alpha ~ normal(0, 1);
  beta ~ normal(0, 1);
  delta ~ normal(.75, 1);
  for (n in 1 : N) 
    y[n] ~ bernoulli_logit(alpha[jj[n]] - beta[kk[n]] + delta);
}

