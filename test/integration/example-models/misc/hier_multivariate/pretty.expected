data {
  int<lower=0> N;
  int<lower=1> K;
  int<lower=1> J;
  int<lower=1> L;
  int<lower=1, upper=J> jj[N];
  matrix[N, K] x;
  matrix[J, L] u;
  vector[N] y;
}
parameters {
  corr_matrix[K] Omega;
  vector<lower=0>[K] tau;
  matrix[L, K] gamma;
  vector[K] beta[J];
  real<lower=0> sigma;
}
model {
  matrix[K, K] Sigma_beta;
  Sigma_beta <- diag_matrix(tau) * Omega * diag_matrix(tau);
  tau ~ cauchy(0, 2.5);
  Omega ~ lkj_corr(2);
  for (l in 1 : L) 
    gamma[l] ~ normal(0, 5);
  for (j in 1 : J) 
    beta[j] ~ multi_normal((u[j] * gamma)', Sigma_beta);
  for (n in 1 : N) 
    y[n] ~ normal(x[n] * beta[jj[n]], sigma);
}


