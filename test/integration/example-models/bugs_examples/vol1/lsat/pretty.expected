data {
  int<lower=0> N;
  int<lower=0> R;
  int<lower=0> T;
  int<lower=0> culm[R];
  int<lower=0> response[R, T];
}
transformed data {
  int r[T, N];
  vector[N] ones;
  for (j in 1 : culm[1]) {
    for (k in 1 : T) {
      r[k, j] <- response[1, k];
    }
  }
  for (i in 2 : R) {
    for (j in (culm[i - 1] + 1) : culm[i]) {
      for (k in 1 : T) {
        r[k, j] <- response[i, k];
      }
    }
  }
  for (i in 1 : N) 
    ones[i] <- 1.0;
}
parameters {
  real alpha[T];
  vector[N] theta;
  real<lower=0> beta;
}
model {
  alpha ~ normal(0, 100.);
  theta ~ normal(0, 1);
  beta ~ normal(0.0, 100.);
  for (k in 1 : T) 
    r[k] ~ bernoulli_logit(beta * theta - alpha[k] * ones);
}
generated quantities {
  real mean_alpha;
  real a[T];
  mean_alpha <- mean(alpha);
  for (t in 1 : T) 
    a[t] <- alpha[t] - mean_alpha;
}


