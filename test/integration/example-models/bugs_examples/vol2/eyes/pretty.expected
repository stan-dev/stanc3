data {
  int<lower=0> N;
  array[N] real y;
}
parameters {
  real<lower=0> sigmasq;
  real<lower=0> theta;
  real lambda_1;
  real<lower=0, upper=1> p1;
}
transformed parameters {
  array[2] real lambda;
  real sigma;
  sigma <- sqrt(sigmasq);
  lambda[1] <- lambda_1;
  lambda[2] <- lambda[1] + theta;
}
model {
  theta ~ normal(0, 100);
  lambda_1 ~ normal(0, 1e3);
  sigmasq ~ inv_gamma(1e-3, 1e-3);
  {
    real log_p1;
    real log1m_p1;
    log_p1 <- log(p1);
    log1m_p1 <- log1m(p1);
    for (n in 1 : N) 
      increment_log_prob(log_sum_exp(log_p1
                                     + normal_log(y[n], lambda[1], sigma),
                                     log1m_p1
                                     + normal_log(y[n], lambda[2], sigma)));
  }
}


