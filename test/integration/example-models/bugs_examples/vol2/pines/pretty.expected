              + gamma_log(tau[1], 0.0001, 0.0001);
  log_p[2] <- log1m(lambda)
              + normal_log(y_std, gamma + delta * z_std, sigma[2])
              + normal_log(gamma, 0, sqrt(1e6))
              + normal_log(delta, 0, sqrt(1e4))
              + gamma_log(tau[2], 0.0001, 0.0001);
  target += log_sum_exp(log_p);
}


