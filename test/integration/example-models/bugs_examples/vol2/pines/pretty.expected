transformed data {
  vector[N] y_std;
  vector[N] x_std;
  vector[N] z_std;
  y_std <- (y - mean(y)) / sd(y);
  x_std <- (x - mean(x)) / sd(x);
  z_std <- (z - mean(z)) / sd(z);
  print("pines-4");
}
parameters {
  real alpha;
  real beta;
  real gamma;
  real delta;
  vector<lower=0>[2] tau;
  real<lower=0, upper=1> lambda;
}
transformed parameters {
  vector<lower=0>[2] sigma;
  for (i in 1 : 2) 
    sigma[i] <- 1 / sqrt(tau[i]);
}
model {
  vector[2] log_p;
  log_p[1] <- log(lambda) + normal_log(y_std, alpha + beta * x_std, sigma[1])
