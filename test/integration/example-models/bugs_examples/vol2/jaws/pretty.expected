  $ ../../../../../../../install/default/bin/stanc --auto_format jaws.stan
data {
  int<lower=0> N;
  int<lower=0> M;
  vector[M] Y[N];
  real age[M];
  cov_matrix[M] S;
}
transformed data {
  real mean_age;
  mean_age <- mean(age);
}
parameters {
  real beta0;
  real beta1;
  cov_matrix[M] Sigma;
}
transformed parameters {
  vector[M] mu;
  for (m in 1 : M) 
    mu[m] <- beta0 + beta1 * age[m];
}
model {
  beta0 ~ normal(0, 32);
  beta1 ~ normal(0, 32);
  Sigma ~ inv_wishart(4, S);
  for (n in 1 : N) 
    Y[n] ~ multi_normal(mu, Sigma);
}


