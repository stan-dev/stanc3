}
parameters {
  real<lower=0> k1;
  real<lower=0> k2;
  real<lower=0> alpha21;
  real<lower=0> alpha12;
  real<lower=0, upper=1> gamma;
  real<lower=0> sigma;
}
transformed parameters {
  real eCO2_hat[N_t];
  eCO2_hat <- evolved_CO2(N_t, t0, ts, gamma, totalC_t0, k1, k2, alpha21,
                          alpha12, x_r, x_i);
}
model {
  gamma ~ beta(10, 1);
  k1 ~ normal(0, 1);
  k2 ~ normal(0, 1);
  alpha21 ~ normal(0, 1);
  alpha12 ~ normal(0, 1);
  sigma ~ cauchy(0, 1);
  for (t in 1 : N_t) 
