  $ ../../../../../../../install/default/bin/stanc --auto_format dawid-skene.stan
data {
  int<lower=2> K;
  int<lower=1> I;
  int<lower=1> J;
  int<lower=1, upper=K> y[I, J];
  vector<lower=0>[K] alpha;
  vector<lower=0>[K] beta[K];
}
parameters {
  simplex[K] pi;
  simplex[K] theta[J, K];
}
model {
  pi ~ dirichlet(alpha);
  for (j in 1 : J) 
    for (k in 1 : K) 
      theta[j, k] ~ dirichlet(beta[k]);
  for (i in 1 : I) {
    vector[K] log_q = log(pi);
    for (j in 1 : J) 
      log_q += to_vector(log(theta[j,  : , y[i, j]]));
    target += log_sum_exp(log_q);
  }
}
generated quantities {
  vector[K] log_Pr_z[I];
  for (i in 1 : I) {
    vector[K] log_q = log(pi);
    for (j in 1 : J) 
      log_q += to_vector(log(theta[j,  : , y[i, j]]));
    log_Pr_z[i] = log_q - log_sum_exp(log_q);
  }
}

