  sigma_t ~ uniform(0, 100);
  rho_yt ~ uniform(-1, 1);
  d ~ normal(0, 31.6);
  b ~ normal(0, 31.6);
  sigma_a ~ uniform(0, 100);
  sigma_g ~ uniform(0, 100);
  rho_ag ~ uniform(-1, 1);
  mu_ag ~ normal(0, 31.6);
  for (j in 1 : J) 
    ag[j] ~ multi_normal(mu_ag, Sigma_ag);
  for (i in 1 : N) 
    yt[i] ~ multi_normal(yt_hat[i], Sigma_yt);
}

