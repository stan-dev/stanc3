model {
  vector[N] y_hat;
  matrix[J, K] B;
  matrix[K, K] rho_b;
  matrix[K, K] Sigma_b_raw;
  vector[K] mu;
  matrix[J, K] B_raw;
  vector[K] sigma_b;
