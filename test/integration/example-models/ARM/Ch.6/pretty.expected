  $ ../../../../../../install/default/bin/stanc --auto-format wells_probit.stan
data {
  int<lower=0> N;
  vector[N] dist;
  array[N] int<lower=0, upper=1> switc;
}
transformed data {
  vector[N] dist100;
  dist100 = dist / 100.0;
}
parameters {
  vector[2] beta;
}
model {
  for (n in 1 : N) 
    switc[n] ~ bernoulli(Phi(beta[1] + beta[2] * dist100[n]));
}

